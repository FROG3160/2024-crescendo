    "version": "0.2.0",

    "configurations": [
        

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Robot Sim",
            "type": "python",
            "request": "launch",
            "module": "robotpy",
            "cwd": "${workspaceFolder}/roborio-src",
            "args": ["sim"],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Robot Deploy",
            "type": "python",
            "request": "launch",
            "module": "robotpy",
            "cwd": "${workspaceFolder}/roborio-src",
            "args": ["deploy", "--skip-tests"],
            "console": "integratedTerminal"
        },
        {
            "name": "Robot Create Physics",
            "type": "python",
            "request": "launch",
            "module": "robotpy",, ,
            "cwd": "${workspaceFolder}/roborio-src",
            "args": ["create-physics"],
            "console": "integratedTerminal"
        }
    ]
}